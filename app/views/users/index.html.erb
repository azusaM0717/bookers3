<div class="container">
  <div class="row">
  <div class="col-md-3">
  <%= render 'info', user: @user %>
  <%= render 'books/new', book: @book %>
  </div>
  <div class="col-md-8 offset-md-1"> 
  <h1>User</h1>
    <% @users.each do |user| %>
      <table>
        <thead>
          <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
          </tr>
        </thead>
        <tbody>
          <tr>
          <td><%= image_tag user.get_profile_image(100,100) %></td>
          <td><%= user.name %></td>
          <td><%= link_to "Show", user_path(user.id) %></td>
          </tr>
        </tbody>
      </table>
    <% end %>
    </div>
  </div>
</div>